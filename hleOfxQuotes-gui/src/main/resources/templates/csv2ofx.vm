OFXHEADER:100
DATA:OFXSGML
VERSION:102
SECURITY:NONE
ENCODING:USASCII
CHARSET:1252
COMPRESSION:NONE
OLDFILEUID:NONE
NEWFILEUID:NONE

<OFX>
    <SIGNONMSGSRSV1>
        <SONRS>
            <STATUS>
                <CODE>0
                <SEVERITY>INFO
            </STATUS>
            <DTSERVER>$DTSERVER
            <LANGUAGE>ENG
            <FI>
                <ORG>$ORG
                <FID>$FID
            </FI>
        </SONRS>
    </SIGNONMSGSRSV1>
    <BANKMSGSRSV1>
        <STMTTRNRS>
            <TRNUID>1
            <STATUS>
                <CODE>0
                <SEVERITY>INFO
            </STATUS>
            <STMTRS>
                <CURDEF>$CURDEF
                <BANKACCTFROM>
                    <BANKID>$BANKID
                    <ACCTID>$ACCTID
                    <ACCTTYPE>$ACCTTYPE
                </BANKACCTFROM>
                <BANKTRANLIST>
                    <DTSTART>$DTSTART
                    <DTEND>$DTEND
#foreach ( $transaction in $transactions )
                    <STMTTRN>
                        <TRNTYPE>$transaction.TRNTYPE
                        <DTPOSTED>$transaction.DTPOSTED
#if ( $transaction.DTUSER )
                        <DTUSER>$transaction.DTUSER
#end                        
                        <TRNAMT>$transaction.TRNAMT
                        <FITID>$transaction.FITID
#if ( $transaction.NAME )
                        <NAME>$transaction.NAME
#end
#if ( $transaction.MEMO )
                        <MEMO>$transaction.MEMO
#end
                    </STMTTRN>
#end
                </BANKTRANLIST>
                <LEDGERBAL>
                    <BALAMT>$ledgerBalance.BALAMT
                    <DTASOF>$ledgerBalance.DTASOF
                </LEDGERBAL>
            </STMTRS>
        </STMTTRNRS>
    </BANKMSGSRSV1>
</OFX>
